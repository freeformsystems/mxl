{
  "name": "mxl",
  "version": "0.4.5",
  "description": "Tmux launcher",
  "author": "muji <noop@xpm.io>",
  "directories": {
    "bin": "./bin"
  },
  "license": "MIT",
  "homepage": "https://github.com/freeformsystems/mxl",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/mxl"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/mxl/issues"
  },
  "keywords": [
    "tmux",
    "launcher",
    "husk",
    "package",
    "cli"
  ],
  "dependencies": {
    "cli-argparse": "~1.0.0",
    "cli-command": "~0.8.296",
    "cli-env": "~1.0.10",
    "cli-interface": "~1.0.17",
    "cli-logger": "~0.5.38",
    "cli-mid-color": "~1.0.7",
    "cli-mid-logger": "~1.0.8",
    "cli-mid-manual": "~1.0.16",
    "cli-property": "0.0.8",
    "cli-rc": "~1.0.11",
    "cli-util": "~1.1.27",
    "ttycolor": "~0.8.14"
  },
  "scripts": {
    "docs": "npm run manual && npm run readme",
    "clean-man": "rm -fv ./doc/man/*.1",
    "manual": "npm run clean-man && NODE_ENV=devel manpage --output ./doc/man ./bin/mxl",
    "readme": "mdp --force -v"
  },
  "mdp": {
    "title": "Tmux Launcher",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/mxl",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "usage.md"
        ]
      },
      {
        "inc": "../../tmux.conf",
        "type": "code",
        "language": "conf"
      },
      {
        "inc": [
          "run.md"
        ]
      },
      {
        "inc": [
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
