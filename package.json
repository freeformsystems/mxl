{
  "name": "mxl",
  "version": "0.1.1",
  "description": "Tmux launcher",
  "author": "muji <noop@xpm.io>",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/freeformsystems/mxl",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/mxl"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/mxl/issues"
  },
  "keywords": [
    "tmux",
    "launcher",
    "husk",
    "package",
    "cli"
  ],
  "dependencies": {
    "husk": "~0.4.14",
    "husk-parse": "~1.0.4"
  },
  "tmux": [
    "new-window -n mxl -c #{pane_current_path}",
    ["send-keys", "-t:", "vim .", "C-m"],
    "split-window -h -t:",
    ["send-keys", "-t:", "git status", "C-m"],
    "split-window -v -t:",
    ["send-keys", "-t:", "npm test", "C-m"]
  ],
  "scripts": {}
}
