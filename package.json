{
  "name": "mxl",
  "version": "0.3.4",
  "description": "Tmux launcher",
  "author": "muji <noop@xpm.io>",
  "bin": "./bin/mxl",
  "license": "MIT",
  "homepage": "https://github.com/freeformsystems/mxl",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/mxl"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/mxl/issues"
  },
  "keywords": [
    "tmux",
    "launcher",
    "husk",
    "package",
    "cli"
  ],
  "dependencies": {
    "cli-argparse": "~1.0.0",
    "cli-env": "~1.0.10",
    "husk": "~0.4.14",
    "husk-parse": "~1.0.4",
    "husk-transform": "~1.0.3",
    "ttycolor": "~0.8.14"
  },
  "tmux": {
    "run": "open",
    "profiles": {
      "open": [
        "new-window -n mxl -c ${mxl_project}",
        [
          "send-keys",
          "-t:",
          "vim .",
          "C-m"
        ],
        "split-window -h -t: -c ${mxl_project}",
        [
          "send-keys",
          "-t:",
          "git status",
          "C-m"
        ],
        "split-window -v -t: -c ${mxl_project}",
        [
          "send-keys",
          "-t:",
          "npm test",
          "C-m"
        ]
      ]
    }
  },
  "scripts": {
    "readme": "mdp --force -v"
  },
  "mdp": {
    "title": "Tmux Launcher",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/mxl",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "usage.md"
        ]
      },
      {
        "ref": "tmux",
        "type": "code",
        "language": "json"
      },
      {
        "inc": [
          "run.md",
          "environment.md",
          "alias.md"
        ]
      },
      {
        "inc": [
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
